{
  "name": "tab-tower",
  "title": "Tab Tower",
  "description": "Advanced browser tabs manager.",
  "author": "Stephen Berquet",
  "version": "0.3.4",
  "main": "dist/src/main.js",
  "license": "MIT",
  "devDependencies": {
    "@types/archiver": "^2.1.0",
    "@types/chai": "^4.0.10",
    "@types/detect-browser": "^2.0.0",
    "@types/fs-extra": "^5.0.0",
    "@types/materialize-css": "^1.0.0",
    "@types/mocha": "^2.2.45",
    "@types/selenium-webdriver": "^3.0.8",
    "@types/semver": "^5.5.0",
    "@types/uuid": "^3.4.3",
    "@types/websocket": "^0.0.35",
    "@types/which": "^1.0.28",
    "archiver": "^2.1.1",
    "awesome-typescript-loader": "^3.4.1",
    "chai": "^4.1.2",
    "child-process-promise": "^2.2.1",
    "concurrently": "^3.5.1",
    "fs-extra": "^5.0.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.2",
    "sass-lint": "^1.12.1",
    "selenium-webdriver": "^3.6.0",
    "semver": "^5.5.0",
    "source-map-support": "^0.5.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "web-ext": "^2.2.2",
    "webpack": "^3.10.0",
    "websocket": "^1.0.25",
    "which": "^1.3.0"
  },
  "scripts": {
    "start": "web-ext run --ignore-files='**/*' -f firefox-developer-edition --bc",
    "prebuild-extension:firefox": "node bin/clean.js && npm run build && tsc -p tsconfig.bin.json",
    "build-extension:firefox": "node ./dist/bin/build-extension.js",
    "build": "npm run build:ts && npm run build:sass",
    "build:ts": "node bin/patch-materialize-css.js && webpack",
    "build:sass": "node-sass src/scss/main.scss dist/css/style.css --source-map true",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Sass\" \"npm run watch:ts\" \"npm run watch:sass\"",
    "watch:ts": "node bin/patch-materialize-css.js && webpack --watch",
    "watch:sass": "npm run build:sass && node-sass -w src/scss/main.scss dist/css/style.css --source-map true",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "tslint -p tsconfig.json",
    "lint:sass": "sass-lint -v -q",
    "pretest": "NODE_ENV=test npm run build-extension:firefox && tsc -p tsconfig.test.json",
    "test": "mocha --require source-map-support/register dist/tests --recursive --timeout 20000 --slow 5000 -b"
  },
  "dependencies": {
    "detect-browser": "^2.1.0",
    "material-design-icons": "^3.0.1",
    "materialize-css": "^0.100.2",
    "moment": "^2.20.1",
    "uuid": "^3.1.0"
  }
}
