{
    "manifest_version": 2,
    "name": "Tab Tower",
    "version": "0.7.0",
    "description": "Enhanced tab experience",
    "applications": {
        "gecko": {
            "id": "tab-tower@stephen.berquet",
            "strict_min_version": "57.0"
        }
    },
    "icons": {
        "16": "icons/tab-tower.svg",
        "32": "icons/tab-tower.svg",
        "48": "icons/tab-tower.svg",
        "64": "icons/tab-tower.svg",
        "96": "icons/tab-tower.svg"
    },
    "browser_action": {
        "browser_style": false,
        "default_title": "Tab Tower",
        "default_icon": {
            "16": "icons/tab-tower.svg",
            "32": "icons/tab-tower.svg",
            "64": "icons/tab-tower.svg"
        }
    },
    "sidebar_action": {
        "browser_style": false,
        "default_panel": "ui/ui-small.html",
        "default_title": "Tab Tower",
        "default_icon": {
            "16": "icons/tab-tower.svg",
            "32": "icons/tab-tower.svg",
            "64": "icons/tab-tower.svg"
        }
    },
    "background": {
        "scripts": [
            "dist/js/background.js"
        ]
    },
    "permissions": [
        "activeTab",
        "sessions",
        "storage",
        "tabs",
        "unlimitedStorage"
    ]
}