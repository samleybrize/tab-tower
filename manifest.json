{
    "manifest_version": 2,
    "name": "Tab Tower",
    "version": "0.2.0",
    "description": "Description",
    "applications": {
        "gecko": {
            "id": "tab-tower@stephen.berquet",
            "strict_min_version": "57.0",
            "strict_max_version": "58.9"
        }
    },
    "icons": {
        "48": "icons/tab-tower.svg",
        "96": "icons/tab-tower.svg"
    },
    "browser_action": {
        "default_icon": {
            "16": "icons/tab-tower.svg",
            "32": "icons/tab-tower.svg",
            "64": "icons/tab-tower.svg"
        },
        "default_title": "Tab Tower"
    },
    "background": {
        "scripts": [
            "dist/js/background.js"
        ]
    },
    "permissions": [
        "activeTab",
        "sessions",
        "storage",
        "tabs",
        "unlimitedStorage"
    ]
}