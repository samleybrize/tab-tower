{
  "name": "tab-tower",
  "version": "1.0.0",
  "main": "dist/src/main.js",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/materialize-css": "^1.0.0",
    "@types/mocha": "^2.2.45",
    "@types/selenium-webdriver": "^3.0.8",
    "@types/uuid": "^3.4.3",
    "@types/websocket": "^0.0.35",
    "@types/which": "^1.0.28",
    "awesome-typescript-loader": "^3.4.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.2",
    "sass-lint": "^1.12.1",
    "selenium-webdriver": "^3.6.0",
    "source-map-support": "^0.5.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "web-ext": "^2.2.2",
    "webpack": "^3.10.0",
    "websocket": "^1.0.25",
    "which": "^1.3.0"
  },
  "scripts": {
    "start": "web-ext run --ignore-files='**/*'",
    "build": "npm run build:ts && npm run build:sass",
    "build:ts": "webpack",
    "build:sass": "node-sass src/scss/main.scss dist/css/style.css --source-map true",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Sass\" \"npm run watch:ts\" \"npm run watch:sass\"",
    "watch:ts": "webpack --watch",
    "watch:sass": "npm run build:sass && node-sass -w src/scss/main.scss dist/css/style.css --source-map true",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "tslint -p tsconfig.json --type-check './src/**/*.ts'",
    "lint:sass": "sass-lint -v -q",
    "pretest": "NODE_ENV=test npm run build && tsc -p tsconfig.test.json",
    "test": "mocha --require source-map-support/register dist/tests --recursive --timeout 20000 --slow 5000 -b"
  },
  "dependencies": {
    "material-design-icons": "^3.0.1",
    "materialize-css": "^0.100.2",
    "uuid": "^3.1.0"
  }
}
